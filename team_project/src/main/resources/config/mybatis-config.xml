<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>
	<!-- 마이바티스 작동 규칙 정의 -->
	<settings>
		<setting name="cacheEnabled" value="false" />
		<setting name="useGeneratedKeys" value="false" />
		<setting name="mapUnderscoreToCamelCase" value="true" />
	</settings>
	<typeAliases>
		<typeAlias alias="memberDto" type="dto.member.MemberDto" />
		<typeAlias alias="noticeDto" type="dto.notice.NoticeDto" />
		<typeAlias alias="qnaDto" type="dto.qna.QnaDto" />
		<typeAlias alias="rcommentDto" type="dto.rcomment.RCommentDto" />
		<typeAlias alias="recipeDto" type="dto.recipe.RecipeDto" />
		<typeAlias alias="talkDto" type="dto.talk.TalkDto" />
		<typeAlias alias="tcommentDto" type="dto.tcomment.TCommentDto" />
	</typeAliases>
</configuration>
